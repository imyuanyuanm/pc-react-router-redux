{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        "@babel/preset-react",
        [
            "@babel/preset-stage-2",
            {
                "loose": true,
                "decoratorsLegacy": true
            }
        ]
    ],
    "plugins": [
        [
            "@babel/transform-runtime",
            {
                "corejs": 2,
                "regenerator": true,
                "absoluteRuntime": "babel-runtime"
            }
        ],
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ]
    ]
}